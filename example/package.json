		"@types/react": "18.2.79",
		"@types/react-dom": "18.2.25",
		"@typescript-eslint/eslint-plugin": "7.7.0",
		"@typescript-eslint/parser": "7.7.0",
		"eslint": "9.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.3",
